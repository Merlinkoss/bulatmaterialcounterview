<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CounterView">
        <attr name="bulat_type">
            <enum name="intValues" value="0" />
            <enum name="floatValues" value="1" />
        </attr>
        <attr name="bulat_maxValue" format="float" />
        <attr name="bulat_minValue" format="float" />
        <attr name="bulat_diff" format="float" />
        <attr name="bulat_value" format="float" />
        <attr name="bulat_secondaryColor" format="color" />
        <attr name="bulat_primaryColor" format="color" />
        <attr name="bulat_rippleColor" format="color" />
        <attr name="bulat_separatorColor" format="color" />
        <attr name="bulat_plusString" format="string" />
        <attr name="bulat_minusString" format="string" />
    </declare-styleable>
</resources>