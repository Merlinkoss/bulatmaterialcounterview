<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CounterView">
        <attr name="type">
            <enum name="intValues" value="0" />
            <enum name="floatValues" value="1" />
        </attr>
        <attr name="maxValue" format="float" />
        <attr name="minValue" format="float" />
        <attr name="secondaryColor" format="color" />
        <attr name="primaryColor" format="color" />
        <attr name="separatorColor" format="color" />
        <attr name="plusString" format="string" />
        <attr name="minusString" format="string" />
    </declare-styleable>
</resources>